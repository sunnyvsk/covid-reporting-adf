							"value": "@dataset().file_name",
							"type": "Expression"
						},
						"folderPath": "ecdc_data",
						"fileSystem": "landingcovidreporting"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
